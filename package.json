    "clean": "rm -rf dist && rm -rf node_modules/.cache",
    "preview": "vite preview"
  },
  "dependencies": {
    "@emotion/styled": "^11.14.1",
    "axios": "^1.11.0",
    "biome": "^0.3.3",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.6.3",
    "zustand": "^5.0.6"
  },
  "devDependencies": {
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.6.0",
    "typescript": "~5.8.3",
    "vite": "^7.0.4"
  }
}
